{
  "name": "full-stack-app",
  "version": "1.0.0",
  "description": "Full stack application with client and server",
  "main": "server/dist/index.js",
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development webpack serve --config webpack.client.js",
    "dev:server": "wrangler dev",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
    "build:server": "wrangler build --env production",
    "start": "node server/dist/index.js",
    "clean": "rimraf client/dist server/dist",
    "dev": "concurrently \"pnpm run dev:client\" \"wrangler dev\"",
    "deploy": "wrangler publish"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250510.0",
    "@swc/core": "^1.3.107",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.8.0",
    "nodemon-webpack-plugin": "^4.8.2",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.1.0",
    "postcss-preset-env": "^9.3.0",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0",
    "webpack-node-externals": "^3.0.0",
    "wrangler": "^4.14.4"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/server": "^4.12.0",
    "@as-integrations/cloudflare-workers": "^1.1.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-ws": "^5.16.2",
    "graphql-yoga": "^5.13.4",
    "openai": "^4.98.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "uuid": "^11.1.0"
  }
}
